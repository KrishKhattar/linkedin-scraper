{
  "manifest_version": 3,
  "name": "Linkedin Scrapper",
  "version": "1.0",
  "description": "A Chrome extension example.",
  "action": {
    "default_title": "Click me!"
  },
  "permissions": ["tabs", "activeTab", "storage", "scripting"],

  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "personNameScapper.js",
        "linkedinLinks.js",
        "dataPair.js",
        "content.js"
      ]
    }
  ]
}
