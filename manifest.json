{
  "manifest_version": 3,
  "name": "LinkedIn Post Extractor",
  "version": "1.0",
  "description": "Extract post names from LinkedIn",
  "permissions": ["tabs", "activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["personNameScapper.js", "content.js"]
    }
  ]
}
